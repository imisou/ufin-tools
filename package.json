{
  "name": "ufin-tools",
  "version": "0.0.1",
  "description": "tools",
  "type": "module",
  "main": "libs/tools.esm.js",
  "types": "libs/index.d.ts",
  "private": false,
  "author": "njjed <497877853@qq.com>",
  "repository": {
    "type": "git",
