    "rimraf": "^3.0.2",
    "rollup": "^4.18.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.8.3",
    "vitepress": "^1.2.2",
    "vite": "^5.2.11"
  },
